{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "~/*": ["./*"],
      "@/*": ["./*"],
      "~~/*": ["./*"],
      "@@/*": ["./*"]
    },
    "forceConsistentCasingInFileNames": false,
  },
  "vueCompilerOptions": {
    "experimentalDisableTemplateSupport": true
  },
  "exclude": ["node_modules", ".nuxt", "dist"]
}
